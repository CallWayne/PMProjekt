<p id="notice"><%= notice %></p>

<h1>Effort Categories</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @effort_categories.each do |effort_category| %>
      <tr>
        <td><%= effort_category.value %></td>
        <td><%= effort_category.description %></td>
        <td><%= link_to 'Show', effort_category %></td>
        <td><%= link_to 'Edit', edit_effort_category_path(effort_category) %></td>
        <td><%= link_to 'Destroy', effort_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Effort Category', new_effort_category_path %>
